import wollok.game.*
import config.*
import objects.*

/* Por acá se empieza a ejecutar */
program main {
	config.apply() // Solo aplica configuración default de wollok game.

	// Los dos elementos importantes, cada 500 ticks se llama al metodo que controla la imagen.
	game.addVisualCharacter(objetoPrueba)
	game.onTick(500, "pepita-anim", {objetoPrueba.ciclarAnimacion()})
	
	game.start()
}